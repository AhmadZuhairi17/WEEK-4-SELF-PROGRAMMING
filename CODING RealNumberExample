package mySelfProgramming;

class RealNumberExample {

	public static void main(String[] args) {
	System.out.println("2.0 * 2.4 + 2.25 * 4.0 / 2.0 is " + (2.0 * 2.4 + 2.25 * 4.0 / 2.0));
	System.out.printf("5.0 * 8.5 + 9.75 / 2.25 -(9.0 * 2.0) + 15.30 / 2.50 * 8.50 + 2.50) is %.2f" , (5.0*8.5+9.75/2.25-(9.0*2.0) + 15.30 / 2.50 * 8.50 + 2.50));
	System.out.println("\n2.0 + 10 / 3 * 2.5 - 6 / 4 is " + (2.0 + 10 / 3 * 2.5 - 6 / 4));
			
	}

}
